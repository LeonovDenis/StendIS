CREATE TABLE exp
(
    ID                    BIGINT AUTO_INCREMENT NOT NULL,
    detectorName          VARCHAR(255)          NULL,
    detectorSerial        VARCHAR(255)          NULL,
    testerName            VARCHAR(255)          NULL,
    startExpDate          datetime              NULL,
    endExpDate            datetime              NULL,
    countDeselPixel       INT                   NULL,
    countDeselPixelInLine INT                   NULL,
    brakTimes             DOUBLE                NULL,
    brakFPUCount          DOUBLE                NULL,
    brakCHannelCount      DOUBLE                NULL,
    mashtab               DOUBLE                NULL,
    vr0                   INT                   NULL,
    vva                   INT                   NULL,
    vu4                   INT                   NULL,
    vuc                   INT                   NULL,
    tInt                  INT                   NULL,
    temp                  INT                   NULL,
    mode                  VARCHAR(255)          NULL,
    dir                   VARCHAR(255)          NULL,
    ccc                   VARCHAR(255)          NULL,
    frameArrayList30      VARCHAR(255)          NULL,
    frameArrayList40      VARCHAR(255)          NULL,
    shum                  DOUBLE                NULL,
    sredZnach30           DOUBLE                NULL,
    sredZnach40           DOUBLE                NULL,
    NEDT                  DOUBLE                NULL,
    CONSTRAINT pk_exp PRIMARY KEY (ID)
);